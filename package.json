{
  "name": "@flyreel/validation",
  "version": "0.1.0",
  "module": "dist/flyreel-validation.esm.js",
  "main": "dist/index.js",
  "engines": {
    "node": ">=10"
  },
  "license": "MIT",
  "author": "",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/flyreel"
  },
  "files": [
    "dist",
    "src"
  ],
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "tsdx build",
    "lint": "eslint '**/*.{js,ts}'",
    "lint:fix": "npm run lint -- --fix",
    "lint:types": "tsc --noEmit",
    "postversion": "git push && git push origin v$npm_package_version",
    "prepare": "tsdx build",
    "prepublishOnly": "npm run lint && npm run lint:types && npm test && npm run build",
    "release": "yarn build && changeset publish",
    "start": "tsdx watch",
    "test": "tsdx test"
  },
  "prettier": {
    "trailingComma": "none",
    "singleQuote": true,
    "semi": false,
    "printWidth": 80,
    "arrowParens": "avoid"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint:types && lint-staged"
    }
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.2.7",
    "@changesets/cli": "^2.12.0",
    "@size-limit/preset-small-lib": "^4.9.1",
    "@types/yup": "0.29.11",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "eslint": "7.16.0",
    "eslint-config-prettier": "7.1.0",
    "husky": "^4.3.6",
    "libphonenumber-js": "1.9.6",
    "tsdx": "^0.14.1",
    "tslib": "^2.0.3",
    "typescript": "^4.1.3",
    "utility-types": "3.10.0",
    "yup": "0.32.8"
  }
}
